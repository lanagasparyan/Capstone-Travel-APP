        "server": "^1.0.29"
    },
    "devDependencies": {
        "@babel/core": "^7.10.2",
        "@babel/preset-env": "^7.10.2",
        "babel-jest": "^24.9.0",
        "babel-loader": "^8.1.0",
        "body-parser": "^1.19.0",
        "bootstrap": "^4.5.0",
        "clean-webpack-plugin": "^3.0.0",
        "cors": "^2.8.5",
        "css-loader": "^3.5.3",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-async-await": "^1.1.0",
        "express-async-errors": "^3.1.1",
        "file-loader": "^4.3.0",
        "html-loader": "^1.1.0",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^24.9.0",
        "jquery": "^3.5.1",
        "mini-css-extract-plugin": "^0.9.0",
        "mocha": "^8.0.1",
        "node-fetch": "^2.6.0",
        "node-sass": "^7.0.0",
        "popper.js": "^1.16.1",
        "sass-loader": "^8.0.2",
        "style-loader": "^1.2.1",
        "supertest": "^4.0.2",
        "url-loader": "^2.2.0",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.9",
        "webpack-dev-server": "^3.11.0",
        "workbox-webpack-plugin": "^4.3.1"
    },
    "jest": {
        "scripts": {
            "test": "mocha"
        },
        "testEnvironment": "node",
        "coveragePathIgnorePatterns": [
            "/node_modules/"
        ],
        "moduleNameMapper": {
            "^.+\\.(css|less|scss)$": "babel-jest"
        }
    }
}
