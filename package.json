{
    "name": "travel-app",
    "version": "1.0.0",
    "description": "A travel app that obtains a desired trip location & date from the user, and displays weather and an image of the location using information obtained from external APIs.",
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lanagasparyan/Capstone-Travel-APP.git"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/lanagasparyan/Capstone-Travel-APP/issues"
    },
    "homepage": "https://github.com/lanagasparyan/Capstone-Travel-APP/blob/master/README.md",
    "scripts": {
        "test": "jest",
        "start": "node ./src/server/server.js",
        "build-dev": "webpack-dev-server --config webpack.dev.js --open",
        "build-prod": "webpack --config webpack.prod.js",
        "webpack": "webpack",
        "dev": "npm run webpack -- --mode development --watch",
        "build": "npm run webpack -- --mode production"
    },
    "author": "Lana Gasparyan",
    "dependencies": {
        "ejs": "^3.1.3",
        "pixabay-api": "^1.0.4",
        "register-service-worker": "^1.7.1",
